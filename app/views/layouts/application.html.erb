		    $("#vtabs > ul > li").removeClass("ui-corner-top").addClass("ui-corner-left");
		    $("#htabs-inner").tabs();
			$("#htabs-inner2").tabs();
			$("#htabs-inner3").tabs();

		});
		});//]]>  

		function login_form_submit(form_id,form_type,form_name){
			var login_params = [form_name+"_username", form_name+"_password"]
			var my_arr = []
			form_submit = true
			if (form_type=="login") {					
				$("#"+form_name+"_login_type")[0].value='login';
				$("#"+form_id+" input[type='text']").each(function(e){
					// if (this.id==form_name+"_username" || this.id==form_name+"_password") {
					if ($.inArray(this.id, login_params) != -1 ) {
						my_arr.push(this.value);
					}					
				});
			} else{					
				$("#"+form_name+"_login_type")[0].value='register';
				$("#"+form_id+" input[id^='"+form_name+"']").each(function(e){
					console.log(this);
					if ($.inArray(this.id, login_params) == -1 ) {
						my_arr.push(this.value);
					}
				});
			};
			for (var i = 0; i < my_arr.length; i++) {
				console.log("value["+i+"] ="+my_arr[i]);
				if(my_arr[i]=="")
				{
					form_submit = false
				}
			};
			console.log("submit="+form_submit);
			if (form_submit) {
				$("#"+form_id).submit();
			} else{
				return false
			};
		}

		</script> 
